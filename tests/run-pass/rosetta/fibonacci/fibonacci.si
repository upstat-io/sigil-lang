// Rosetta Code: Fibonacci sequence
// Task: Compute the nth Fibonacci number

// Single fibonacci with memoization
@fib (n: int) -> int = recurse(
    condition: n <= 1,
    base: n,
    step: self(n - 1) + self(n - 2),
    memo: true
)

