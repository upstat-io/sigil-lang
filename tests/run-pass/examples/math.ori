// Math functions in Ori

@add (a: int, b: int) -> int = a + b

@multiply (a: int, b: int) -> int = a * b

@square (n: int) -> int = n * n

@main () -> void = {
    print(msg: "2 + 3 = " + str(add(a: 2, b: 3)));
    print(msg: "4 * 5 = " + str(multiply(a: 4, b: 5)));
    print(msg: "6^2 = " + str(square(6)))
}
