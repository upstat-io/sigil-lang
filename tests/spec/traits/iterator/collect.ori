// Spec: Section 3.8 — Collect Trait (type-directed collect)
// Tests for basic collect() behavior.

use std.testing { assert, assert_eq }

// =============================================================================
// Default collect → list
// =============================================================================

@collect_default_list () -> [int] = {
    [1, 2, 3].iter().collect()
}

@test_collect_default_list tests @collect_default_list () -> void = {
    assert_eq(actual: collect_default_list(), expected: [1, 2, 3])
}
