// Simple match pattern
// Spec: 10-patterns.md ยง match

@classify (n: int) -> str = match(n,
    0 -> "zero",
    1 -> "one",
    _ -> "other",
)

@option_value (opt) -> int = match(opt,
    Some(x) -> x,
    None -> 0,
)

@result_value (res) -> int = match(res,
    Ok(x) -> x,
    Err(_) -> -1,
)
