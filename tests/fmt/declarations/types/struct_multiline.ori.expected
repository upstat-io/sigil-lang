// Note: Formatter keeps structs inline if they fit in 100 chars
// UserProfile (86 chars) fits inline, Configuration (>100 chars) breaks

type UserProfile = { id: int, name: str, email: str, created_at: int, preferences: str }

type Configuration = {
    database_url: str,
    max_connections: int,
    timeout_seconds: int,
    retry_enabled: bool,
    log_level: str,
}
